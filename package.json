{
  "name": "tourcompanion-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://tourcompanion360.github.io/tourcompanion-dashboard",
  "scripts": {
    "dev": "vite",
    "dev:backend": "node --loader ts-node/esm src/backend/index.js",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev\"",
    "build": "vite build",
    "build:prod": "cross-env NODE_ENV=production vite build",
    "build:dev": "vite build --mode development",
    "build:backend": "tsc --project tsconfig.backend.json",
    "build:full": "npm run build:backend && npm run build",
    "start": "node dist/backend/index.js",
    "start:dev": "nodemon --loader ts-node/esm src/backend/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "echo 'No unit tests configured'",
    "test:integration": "echo 'No integration tests configured'",
    "test:e2e": "echo 'No E2E tests configured'",
    "test:coverage": "echo 'No coverage tests configured'",
    "test:watch": "vitest watch",
    "type-check": "tsc --noEmit",
    "type-check:backend": "tsc --project tsconfig.backend.json --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "check:all": "powershell -ExecutionPolicy Bypass -File .\\scripts\\checks\\check-all.ps1",
    "check:secrets": "powershell -ExecutionPolicy Bypass -File .\\scripts\\checks\\check-secrets.ps1",
    "check:sizes": "powershell -ExecutionPolicy Bypass -File .\\scripts\\checks\\check-file-sizes.ps1",
    "check:duplicates": "powershell -ExecutionPolicy Bypass -File .\\scripts\\checks\\check-duplicates.ps1",
    "check:deps": "powershell -ExecutionPolicy Bypass -File .\\scripts\\checks\\check-dependencies.ps1",
    "create-test-user": "node scripts/create-test-user.js",
    "setup-dev": "node scripts/setup-dev-env.js",
    "quick-start": "node scripts/quick-start.js",
    "debug-auth": "node scripts/debug-auth.js",
    "set-tester": "node scripts/set-tester.js",
    "netlify:login": "netlify login",
    "netlify:init": "netlify init",
    "netlify:dev": "netlify dev",
    "netlify:build": "netlify build",
    "netlify:deploy": "netlify deploy --dir=dist",
    "netlify:deploy:prod": "netlify deploy --dir=dist --prod",
    "vercel:login": "vercel login",
    "vercel:dev": "vercel dev",
    "vercel:build": "vercel build",
    "vercel:deploy": "vercel --prod",
    "deploy": "npm run build && gh-pages -d dist",
    "deploy:github": "npm run build && gh-pages -d dist",
    "predeploy": "npm run build"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/supabase-js": "^2.47.10",
    "@tanstack/react-query": "^5.62.8",
    "@tanstack/react-query-devtools": "^5.62.8",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dompurify": "^3.1.7",
    "dotenv": "^16.4.5",
    "embla-carousel-react": "^8.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.2.0",
    "input-otp": "^1.4.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.469.0",
    "next-themes": "^0.3.0",
    "papaparse": "^5.4.1",
    "react": "^18.3.1",
    "react-day-picker": "^8.9.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-resizable-panels": "^2.0.22",
    "react-router-dom": "^6.30.1",
    "recharts": "^2.15.4",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.9",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^22.16.5",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/xlsx": "^0.0.35",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/coverage-v8": "^1.0.4",
    "audit-ci": "^7.1.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^15.15.0",
    "jsdom": "^23.0.1",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^5.4.19",
    "vitest": "^1.0.4"
  }
}
